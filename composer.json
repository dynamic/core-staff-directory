{
    "name": "dynamic/core-staff-directory",
    "description": "The Dynamic Core Staff Directory Module",
    "authors": [
        {
            "name": "Dynamic Inc",
            "email": "dev@dynamicagency.com",
            "homepage": "http://dynamicagency.com"
        }
    ],
    "keywords": ["silverstripe", "module"],
