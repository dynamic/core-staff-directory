{
    "name": "dynamic/core-staff-directory",
    "description": "The Dynamic Core Staff Directory Module",
    "authors": [
        {
            "name": "Dynamic Inc",
            "email": "dev@dynamicagency.com",
            "homepage": "http://dynamicagency.com"
        }
    ],
    "keywords": [
        "silverstripe",
        "module"
    ],
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "require": {
        "silverstripe/recipe-cms": "^1.0",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Staff\\": "src/",
            "Dynamic\\Staff\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-1.0": "1.0.x-dev",
            "dev-master": "2.0.x-dev"
        }
    }
}
